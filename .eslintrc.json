{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "plugins": ["react"],
    "rules": {
        "react/destructuring-assignment":[0],
        "react/prefer-stateless-function":[0],
        "import/prefer-default-export":[0],
        "no-underscore-dangle":[0]
    }
}
